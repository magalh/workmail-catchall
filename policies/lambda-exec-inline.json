{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowSendEmail",
      "Effect": "Allow",
      "Action": "ses:SendRawEmail",
      "Resource": "*"
    },
    {
      "Sid": "AllowS3ReadInbound",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::ses-inbound-bucket-example",
        "arn:aws:s3:::ses-inbound-bucket-example/*"
      ]
    },
    {
      "Sid": "AllowCloudWatchLogging",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "*"
    }
  ]
}
