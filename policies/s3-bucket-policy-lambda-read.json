{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowLambdaReadInbound",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::123456789012:role/lambda-ses-catchall-role"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::ses-inbound-bucket-example",
        "arn:aws:s3:::ses-inbound-bucket-example/*"
      ]
    }
  ]
}
